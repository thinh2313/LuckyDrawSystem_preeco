      #iddetail {
          z-index: 1051;
          /*animation: shake 3s infinite;*/
      }

      .congraterow1 {
          /*background: linear-gradient(to bottom, var(--pink-2), var(--pink-1));*/
          font-size: 30px !important;
          -webkit-background-clip: text;
          font-weight: bold;
          color: var(--pink-1);
          text-shadow: 0px 4px 3px #ffffff;
          text-transform: uppercase;
          animation: congrate 2s infinite ease-out;
          animation-delay: 1s;
      }

      .congraterow2 {
          background: var(--pink-2);
          font-size: 36px !important;
          -webkit-background-clip: text;
          font-weight: bold;
          color: var(--pink-2);
          text-shadow: 0px 4px 3px #ffffff;
          text-transform: uppercase;
          animation: congrate 2s infinite ease-out;
          animation-delay: 1.5s;
      }

      option {
          background: #f6adcd;
          padding: 5px;
      }

          option ::selection {
              background: var(--pink-border);
              cursor: pointer
          }

      @@keyframes shake {
          0% {
              transform: rotate(-10deg)scale(1);
          }

          25% {
              transform: rotate(0deg) scale(1.2);
          }

          50% {
              transform: rotate(10deg)scale(1);
          }

          75% {
              transform: rotate(0deg) scale(1.2)
          }

          100% {
              transform: rotate(-10deg)scale(1)
          }
      }

      .modal {
          background-color: rgb(0 0 51 / 66%) !important;
      }

      .square {
          background: -webkit-linear-gradient(#f29eee, var(--pink-1));
          -webkit-background-clip: text;
          -webkit-text-fill-color: transparent;
          border: 2px solid white !important;
          box-shadow: inset 0px 2px 17px 4px var(--square-shadow) !important;
          text-shadow: 5px 0px 2px rgb(255 255 255 / 30%);
      }

      .bubble {
          /*box-shadow: 0 5px 10px white;*/
          padding: 5px 0;
          color: white;
          min-width: 250px;
          margin: 10px 7px 0px 7px;
          font-size: 26px;
          border: 4px solid var(--pink-border);
          border-radius: 50px;
          background: linear-gradient(170deg, hsla(0, 0%, 100%, 1) 0%, hsla(333, 82%, 72%, 1) 100%);
          text-shadow: 0 3px 3px #a9a8a8;
          background-image: linear-gradient(to top left, var(--pink-2),transparent 35%, transparent 63%,white), linear-gradient(to top, var(--pink-2) 0%, hsl(300, 100%, 90%) 95%);
      }

